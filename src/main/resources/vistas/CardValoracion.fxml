<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="card" maxWidth="Infinity" minWidth="-Infinity" prefHeight="47.0" spacing="8" styleClass="card" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1">
   <VBox prefHeight="200.0" HBox.hgrow="ALWAYS">
      <children>
         <HBox alignment="CENTER_LEFT" prefHeight="43.0" prefWidth="341.0" VBox.vgrow="ALWAYS">
            <children>
            
                      <ImageView fx:id="imgCardValora" fitHeight="40.0" fitWidth="40.0" preserveRatio="true">
                  <HBox.margin>
                     <Insets bottom="10.0" right="10.0" top="10.0" />
                  </HBox.margin>
               </ImageView>
                <Label fx:id="lblNombreCardValora" prefHeight="17.0" prefWidth="81.0" styleClass="titulo-receta" text="Nombre usuario" wrapText="true" HBox.hgrow="ALWAYS" />
               <Region prefHeight="60.0" prefWidth="156.0" HBox.hgrow="ALWAYS" />
               <HBox alignment="CENTER_RIGHT" prefHeight="60.0" prefWidth="188.0">
                  <children>
                     <Label text="Valoracion:">
                        <HBox.margin>
                           <Insets right="10.0" />
                        </HBox.margin>
                     </Label>
                     <Label fx:id="lblPuntuacionCardValora" text="Label" />
                  </children>
               </HBox>
            </children>
         </HBox>
           <VBox fx:id="comentarioCajaValora" alignment="CENTER_LEFT" prefHeight="58.0" prefWidth="321.0" VBox.vgrow="ALWAYS" maxHeight="Infinity">
