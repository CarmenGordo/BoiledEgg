               <children>
                     <Label prefHeight="15.0" prefWidth="73.0" styleClass="lblTipoCard" text="Comentario:" />
                  <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                  <Label fx:id="lblFechaCardValora" alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="126.0" text="Fecha" wrapText="true" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <Label fx:id="lblComentarioCardValora" maxHeight="Infinity" maxWidth="Infinity" prefHeight="15.0" prefWidth="73.0" styleClass="lblTipoCard" text="Comentario:"  />
            <VBox.margin>
               <Insets />
            </VBox.margin>
           </VBox>
      </children>
   </VBox>
</HBox>
